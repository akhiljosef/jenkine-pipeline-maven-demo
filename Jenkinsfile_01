#!groovy

pipeline {
    agent any
    environment {
    PATH = "D:\apache-maven-3.8.4\bin:${env.PATH}"
    }
    stages {
        stage("Build") {
            steps {
                bat "cmd /c mvn -version"
                bat "cmd /c mvn clean install"
            }
        }
    }

    post {
        always {
            cleanWs()
        }
    }
}